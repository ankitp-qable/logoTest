<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="settings1RowModel" type="com.ankitsapplication.app.modules.settings.data.model.Settings1RowModel"/>
    </data>
    <LinearLayout android:clickable="true"
        android:id="@+id/linearPersonality"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStylegray_51cornerRadius">
        <TextView android:alpha="0.6" android:ellipsize="end"
            android:id="@+id/txtPersonality"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_21pxv"
            android:layout_marginStart="@dimen/_122pxh"
            android:layout_marginTop="@dimen/_21pxv"
            android:layout_weight="0.96" android:layout_width="0dp"
            android:singleLine="true"
            android:text="@{settings1RowModel.txtPersonality}"
            style="@style/txtPoppinsmedium12" tools:text="@string/lbl_personality"/>
        <ImageView android:id="@+id/imageArrow"
            android:layout_height="@dimen/_6pxh"
            android:layout_marginBottom="@dimen/_27pxv"
            android:layout_marginEnd="@dimen/_28pxh"
            android:layout_marginStart="@dimen/_91pxh"
            android:layout_marginTop="@dimen/_27pxv"
            android:layout_width="@dimen/_3pxh"
            android:scaleType="fitXY" android:src="@drawable/img_arrow"/>
    </LinearLayout>
</layout>
